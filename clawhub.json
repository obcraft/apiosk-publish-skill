{
  "name": "apiosk-publish",
  "displayName": "Apiosk Publisher",
  "version": "1.2.0",
  "description": "Publish and manage your own APIs on the Apiosk marketplace. Register your API endpoint, set pricing, and start earning from every request.",
  "category": "api",
  "tags": [
    "api",
    "marketplace",
    "monetization",
    "web3",
    "apiosk"
  ],
  "author": {
    "name": "Apiosk",
    "email": "hello@apiosk.com",
    "url": "https://apiosk.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apiosk/apiosk-publish-skill"
  },
  "requires": {
    "bins": ["curl", "jq"]
  },
  "scripts": {
    "register-api": "./register-api.sh",
    "my-apis": "./my-apis.sh",
    "update-api": "./update-api.sh",
    "delete-api": "./delete-api.sh",
    "test-api": "./test-api.sh"
  },
  "security": {
    "level": "benign",
    "no_curl_bash": true,
    "requires_wallet": true,
    "data_access": [
